<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import java.lang.String?>
<AnchorPane fx:id="phoneTreeManagementActionsRootPane" prefHeight="920.0" prefWidth="200.0"
            styleClass="main-action-nav-bar" stylesheets="@../../styles/Styles.css"
            xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="org.rssb.phonetree.controller.phonetreemanagement.PhoneTreeManagementActionsController">
    <children>
        <VBox layoutX="78.0" layoutY="143.0" spacing="25.0" AnchorPane.bottomAnchor="5.0"
              AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="5.0">
            <children>
                <JFXButton fx:id="dashboardButton" buttonType="RAISED" contentDisplay="TOP"
                           onAction="#showDashboard" prefHeight="20.0" prefWidth="150.0"
                           styleClass="main-action-button" text="Dashboard" textAlignment="CENTER" wrapText="true">
                    <graphic>
                        <FontAwesomeIconView glyphName="BAR_CHART" size="24"/>
                    </graphic>
                    <cursor>
                        <Cursor fx:constant="HAND" />
                    </cursor>
                </JFXButton>
                <JFXButton fx:id="teamManagementButton" buttonType="RAISED" contentDisplay="TOP"
                           layoutX="30.0" layoutY="30.0" onAction="#showTeamManagement" prefHeight="20.0"
                           prefWidth="150.0" text="Team" textAlignment="CENTER" wrapText="true">
                    <graphic>
                        <FontAwesomeIconView glyphName="SITEMAP" size="24"/>
                    </graphic>
                    <styleClass>
                        <String fx:value="main-action-button"/>
                    </styleClass>
                    <cursor>
                        <Cursor fx:constant="HAND" />
                    </cursor>
                </JFXButton>
                <JFXButton fx:id="familyManagementButton" buttonType="RAISED" contentDisplay="TOP"
                           layoutX="30.0" layoutY="201.0" onAction="#showFamilyManagement" prefHeight="20.0"
                           prefWidth="150.0" text="Family" textAlignment="CENTER" wrapText="true">
                    <graphic>
                        <FontAwesomeIconView glyphName="GROUP" size="24"/>
                    </graphic>
                    <styleClass>
                        <String fx:value="main-action-button"/>
                    </styleClass>
                    <cursor>
                        <Cursor fx:constant="HAND" />
                    </cursor>
                </JFXButton>
                <JFXButton fx:id="sevadarsPhoneTreeListButton" buttonType="RAISED" contentDisplay="TOP"
                           layoutX="30.0" layoutY="372.0" onAction="#showSevadarsPhoneTreeLists" prefHeight="20.0"
                           prefWidth="150.0" text="Sevadars List" textAlignment="CENTER" wrapText="true">
                    <graphic>
                        <FontAwesomeIconView glyphName="LIST_UL" size="24"/>
                    </graphic>
                    <styleClass>
                        <String fx:value="main-action-button"/>
                    </styleClass>
                    <cursor>
                        <Cursor fx:constant="HAND" />
                    </cursor>
                </JFXButton>
                <JFXButton fx:id="reportManagementButton" buttonType="RAISED" contentDisplay="TOP"
                           layoutX="30.0" layoutY="543.0" onAction="#createReports" prefHeight="20.0"
                           prefWidth="150.0" text="Reports" textAlignment="CENTER" wrapText="true">
                    <graphic>
                        <FontAwesomeIconView glyphName="FILES_ALT" size="24"/>
                    </graphic>
                    <styleClass>
                        <String fx:value="main-action-button"/>
                    </styleClass>
                    <cursor>
                        <Cursor fx:constant="HAND" />
                    </cursor>
                </JFXButton>
                <JFXButton fx:id="teamChartButton" buttonType="RAISED" contentDisplay="TOP"
                           layoutX="30.0" layoutY="696.0" onAction="#createTeamChart" prefHeight="20.0"
                           prefWidth="150.0" text="Team Chart" textAlignment="CENTER" wrapText="true">
                    <graphic>
                        <FontAwesomeIconView glyphName="INDENT" size="24"/>
                    </graphic>
                    <styleClass>
                        <String fx:value="main-action-button"/>
                    </styleClass>
                    <cursor>
                        <Cursor fx:constant="HAND" />
                    </cursor>
                </JFXButton>
                <JFXButton fx:id="snvReportButton" buttonType="RAISED" contentDisplay="TOP" layoutX="30.0"
                           layoutY="474.0" onAction="#prepareSNVReports" prefHeight="20.0" prefWidth="150.0"
                           text="SNV Report" textAlignment="CENTER" wrapText="true">
                    <graphic>
                        <FontAwesomeIconView glyphName="FILES_ALT" size="24"/>
                    </graphic>
                    <styleClass>
                        <String fx:value="main-action-button"/>
                    </styleClass>
                    <cursor>
                        <Cursor fx:constant="HAND" />
                    </cursor>
                </JFXButton>
                <JFXButton fx:id="vacationPlanButton" buttonType="RAISED" contentDisplay="TOP" layoutX="30.0"
                           layoutY="982.0" onAction="#showSevadarsAvailability" prefHeight="20.0" prefWidth="150.0"
                           text="Vacation" textAlignment="CENTER" wrapText="true">
                    <graphic>
                        <FontAwesomeIconView glyphName="FIGHTER_JET" size="24"/>
                    </graphic>
                    <styleClass>
                        <String fx:value="main-action-button"/>
                    </styleClass>
                    <cursor>
                        <Cursor fx:constant="HAND" />
                    </cursor>
                </JFXButton>

                <JFXButton fx:id="administrationButton" buttonType="RAISED" contentDisplay="TOP" layoutX="30.0"
                           layoutY="982.0" onAction="#showAdminScreen" prefHeight="20.0" prefWidth="150.0"
                           text="Admin" textAlignment="CENTER" wrapText="true">
                    <graphic>
                        <FontAwesomeIconView glyphName="GEARS" size="24"/>
                    </graphic>
                    <styleClass>
                        <String fx:value="main-action-button"/>
                    </styleClass>
                    <cursor>
                        <Cursor fx:constant="HAND" />
                    </cursor>
                </JFXButton>

            </children>
            <padding>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
            </padding>
        </VBox>
    </children>
</AnchorPane>
